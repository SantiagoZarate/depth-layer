{
  "name": "depth-decorator",
  "private": false,
  "version": "0.0.18",
  "type": "module",
  "description": "react component to add depth looking style to your own components",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/depth-decorator.js",
      "require": "./dist/depth-decorator.umd.js",
      "styles": "./dist/style.css"
    }
  },
  "main": "dist/depth-decorator.js",
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "css": "dist/style.css",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Santiago Zarate",
    "email": "santiago2001zarate@gmail.com"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/SantiagoZarate/depth-layer.git"
  },
  "keywords": [
    "React",
    "Component",
    "Depth Layer",
    "Tailwindcss",
    "Typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "test": "vitest --run",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:staged": "lint-staged",
    "prepare": "husky",
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "tailwind-merge": "2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@testing-library/react": "14.2.1",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "eslint-plugin-storybook": "0.6.15",
    "happy-dom": "13.3.8",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "postcss": "8.4.32",
    "rollup-plugin-typescript2": "0.36.0",
    "storybook": "^7.6.17",
    "tailwindcss": "3.3.6",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-plugin-dts": "3.6.4",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.2.2"
